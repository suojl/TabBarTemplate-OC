platform :ios,'8.0'
inhibit_all_warnings!

source 'https://github.com/CocoaPods/Specs.git'
source 'https://git.coding.net/suojl/PrivateCocoapods.git'
#source 'https://git.coding.net/LiuXiaoZhuang/HomePageModule.git'

target 'TabBarTemplate-OC' do
#键盘自动调整框架
    pod 'IQKeyboardManager’

#    pod 'CWStatusBarNotification'
#数据库操作
    pod 'FMDB', :inhibit_warnings => true
#网络状态监测
    pod 'Reachability', :inhibit_warnings => true

#显示提示信息
    pod 'AFMInfoBanner', :inhibit_warnings => true
    pod 'MBProgressHUD', :inhibit_warnings => true

#label数字自动增长
    pod 'UICountingLabel' , :inhibit_warnings => true
    #发送邮件
    pod 'skpsmtpmessage'


    pod 'AFNetworking'
    pod 'SDWebImage', '~>3.8'
    pod 'Masonry'
    
    pod 'ReactiveObjC', '~> 3.1.0'
    # 优化TableView的组件
    pod 'UITableView+FDTemplateLayoutCell', '~> 1.6'
    #管理日志组件
    pod 'CocoaLumberjack'
    #日期选择控件
    pod 'FSCalendar'
    #动画组件
    pod 'pop'
    
    #私有组件
    pod 'MyCocoapods'
end

target 'TabBarTemplate-OCTests' do
    #数据库操作
    pod 'FMDB'
end

#Using AFMInfoBanner (1.2.1)
#Using AFNetworking (3.1.0)
#Using CVKHierarchySearcher (0.2.0)
#Using CWStatusBarNotification (2.3.4)
#Using FMDB (2.6.2)
#Using IQKeyboardManager (4.0.2)
#Using MBProgressHUD (0.9.2)
#Using Reachability (3.2)
